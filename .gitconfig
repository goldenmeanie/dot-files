[core]
	editor = subl -w
	autocrlf = input
	excludesfile = /Users/alex/.gitignore-global
[user]
	name = Alex Taylor
	email = alex@goldenmeanie.net
[color]
    ui = auto
  [color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
  [color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
  [color "status"]
    added = green
    changed = red
    untracked = cyan
[alias]
	co = checkout
[push]
	default = tracking
[rebase]
	stat = true
[rerere]
	enabled = 1
